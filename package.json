    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "http-server": "^14.1.1",
    "multer": "^2.0.2",
    "pg": "^8.16.2"
